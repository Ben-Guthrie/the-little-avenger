[sub_resource type="GDScript" id="GDScript_evwn0"]
script/source = "extends \"res://triggers/dialogue_trigger.gd\"


func trigger_effect(target: Node):
	target.play_idle(Vector2.UP)
	SignalBus.pause_animation.emit()
	DialogueManager.show_dialogue_balloon(dialogue_resource, title, [self])
	await DialogueManager.dialogue_ended
	SignalBus.resume_animation.emit()
	complete.emit()
"

[sub_resource type="GDScript" id="GDScript_qbmfl"]
script/source = "extends \"res://triggers/dialogue_trigger.gd\"


func after_animation():
	var timer = Timer.new()
	add_child(timer)
	timer.start(0.5)
	await timer.timeout
	var timmy = get_parent().get_parent().get_node(\"Characters/Timmy\")
	timmy.play_idle(Vector2.RIGHT)
	timer.start(0.5)
	await timer.timeout
	
	Game.pathfinder.add_point_at_tile(Game.pathfinder.world_to_tile($\"../../Paths/Timmy/Marker2D5\".global_position))
"

[sub_resource type="GDScript" id="GDScript_og4uc"]
script/source = "extends Trigger


func trigger_effect(_target):
	var timer = Timer.new()
	add_child(timer)
	timer.start(0.5)
	await timer.timeout
	var ghost = $\"../../Characters/Ghost\"
	ghost.flip_h = true
	timer.start(1)
	await timer.timeout
	$AnimationPlayer.play(\"fade\")
	await $AnimationPlayer.animation_finished
	timer.start(1)
	await timer.timeout
	Game.main_scene.end_game()
"

